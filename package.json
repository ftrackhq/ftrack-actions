{
  "name": "@ftrack/actions",
  "description": "GitHub actions for ftrack",
  "type": "module",
  "private": true,
  "workspaces": [
    "actions/*"
  ],
  "scripts": {
    "test": "yarn lint && vitest --run",
    "lint": "eslint . && prettier -c ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ftrackhq/actions.git"
  },
  "keywords": [],
  "homepage": "http://ftrack.com",
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json,jsx,scss,yml}": "prettier --write"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.54.0",
    "msw": "1.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "packageManager": "yarn@4.0.2"
}
